: fixed;\n            top: 0;\n            right: 0;\n            padding-right: 60px;\n            display: flex;\n            justify-content: flex-end;\n            align-items: center;\n            z-index: 10000;\n            transition: all .5s ease-in-out;\n            \n        }\n\n        .ProjectNav {\n            width: 100%;\n            height: 120px;\n            position: fixed;\n            top: 0;\n            right: 0;\n            padding: 0 40px;\n            display: flex;\n            justify-content: space-between;;\n            align-items: center;\n            z-index: 10000;\n            transition: all .5s ease-in-out;\n        }\n\n            .Portfolio-Logo {\n                height: 120px;\n                position: fixed;\n                top: 0;\n                left: 0;\n                padding-left: 40px;\n                display: flex;\n                justify-content: flex-end;\n                align-items: center;\n                z-index: 10000;\n                transition: all .5s ease-in-out;\n            }\n\n            .NavList {\n                display: flex;\n                justify-content: space-between;\n                width: 180px;\n                transition: width .5s ease-in-out;\n            }\n\n                .NavItem {\n                }\n\n                    .NavLink {\n                        color: #E6E6E6;\n                        font-size: 15px;\n                        font-weight: 200;\n                        text-transform: uppercase;\n                        transition: font-size  .5s ease-in-out;\n                    }\n\n                        .NavLink:hover {\n                            color: #c0dde1;\n                        }\n\n                        #NavLinkPortfolio {\n                            font-weight: 900;\n                            \n                        }\n\n                        #NavLinkAbout {\n                            font-weight: 900;\n                        }\n\n        \n\n        @media screen and (max-width: 650px){\n            .Nav {\n                padding-right: 35px;\n            }\n\n            .ProjectNav {\n                width: 100%;\n                padding: 0 35px;\n            }\n            .NavList {\n            }\n\n            .NavLink {\n                font-size: 16px;\n            }\n            \n        }\n\n/*\n-\n-\n-\n-\n-\n-\n-\n-\n-\n-\n-\n*/\n\n/* -- FOOTER ----- */\n            .Footer {\n                width: 100%;\n                height: 30px;\n                position: absolute;\n                bottom: 0;\n                left: 0;\n                padding-left: 10%;\n                display: flex;\n                justify-content: flex-start;\n                align-items: center;\n            }\n\n                .FooterText {\n                    color: #141414;\n                    text-transform: capitalize;\n                    font-size: 14px;\n                    font-weight: 200;\n                    color: #E6E6E6;\n                    white-space: nowrap;\n                }\n\n                .FooterText2Wrap {\n                    display: flex;\n                    align-items: center;\n                    height: 25px;\n                }\n\n                    #FooterText2 {\n                        margin-left: 140px;\n                    }\n\n                .Bold {\n                    font-weight: 700;\n                }\n\n                .HeartIcon {\n                    width: 15px;\n                    height: 12px;\n                    margin: 0 3px;\n                    background: url('../Assets/Icons/HeartIcon.svg');\n                    background-position: center;\n                    background-size: contain;\n                    background-repeat: no-repeat;\n                }\n\n            @media screen and (max-width: 780px){\n                .Footer {\n                    height: 60px;\n                    flex-direction: column-reverse;\n                    align-items: flex-start;\n                    justify-content: center;\n                }\n\n                .FooterText {\n                    font-size: 12px;\n                }\n                #FooterText2 {\n                    margin-left: 0;\n                }\n                #FooterText1 {\n                    margin-left: 0;\n                }\n            }"},"lineCount":null}},"error":null,"hash":"df5399cc0f5894b14239f6fcf0afb591","cacheData":{"env":{}}}